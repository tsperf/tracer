{
  "publisher": "tsperf",
  "name": "tracer",
  "displayName": "Type Complexity Tracer",
  "version": "0.1.2",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "description": "A VSCode extension to measure type complexity within a project.",
  "license": "MIT",
  "homepage": "https://github.com/tsperf/tracer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsperf/tracer"
  },
  "bugs": {
    "url": "https://github.com/tsperf/tracer/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onLanguage:typescript",
    "workspaceContains:tsconfig.json"
  ],
  "contributes": {
    "configuration": {
      "title": "tsperf.tracer",
      "properties": {
        "tsperf.tracer.checkTimeWarning": {
          "type": "number",
          "default": 1,
          "description": "Warn when type check times exceed this number of seconds"
        },
        "tsperf.tracer.port": {
          "type": "number",
          "default": 3000,
          "description": "port to run local web server on"
        }
      }
    },
    "commands": [
      {
        "command": "tsperf.tracer.openInBrowser",
        "title": "Open trace view in browser",
        "icon": {
          "dark": "resources/todo.svg",
          "light": "resources/todo.svg"
        }
      },
      {
        "command": "tsperf.tracer.runTrace",
        "title": "tsc trace",
        "icon": {
          "dark": "resources/todo.svg",
          "light": "resources/todo.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build-spa  ",
    "compile": "esbuild --bundle --outfile=dist/index.js  --platform=node --external:vscode src/index.ts",
    "dev": "esbuild --watch --bundle --outfile=dist/index.js  --platform=node --external:vscode src/index.ts",
    "build-spa": "cd spa && quasar build && cd .. &&  mkdir -p dist/static && cp -pr spa/dist/spa/* dist/static/ ",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "t-build": "rollup -c",
    "t-dev": "nr build --watch",
    "lint": "eslint .",
    "t-vscode:prepublish": "nr build",
    "t-publish": "vsce publish --no-dependencies",
    "t-package": "vsce package --no-dependencies",
    "t-test": "vitest",
    "typecheck": "tsc --noEmit && pnpm --filter ui typecheck",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@antfu/ni": "^0.21.12",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/d3": "^7.4.3",
    "@types/node": "^18.19.33",
    "@types/vscode": "^1.70.0",
    "@vscode/vsce": "^2.26.1",
    "bumpp": "^9.4.1",
    "d3": "^7.9.0",
    "d3-flame-graph": "^4.1.3",
    "eslint": "^9.2.0",
    "open": "^10.1.0",
    "perfect-debounce": "^1.0.0",
    "rollup": "^4.17.2",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@fastify/websocket": "^10.0.1",
    "@trpc/client": "11.0.0-rc.366",
    "@trpc/server": "11.0.0-rc.366",
    "fastify": "^4.27.0"
  }
}